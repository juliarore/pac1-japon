{
  "name": "pac1-japon",
  "version": "1.0.0",
  "description": "Descubriendo Japón en 15 días",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist .cache .cache-loader .parcel-cache",
    "parcel:serve": "parcel -p 8123 --target web",
    "parcel:build": "parcel build --target web --no-source-maps --no-cache",
    "dev": "npm-run-all clean parcel:serve",
    "build": "npm-run-all clean parcel:build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "targets": {
    "web": {
      "context": "browser",
      "publicUrl": "./",
      "source": "src/index.html",
      "engines": {
        "browsers": "last 2 versions, > 0.5%, not dead"
      }
    }
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/juliarore/pac1-japon"
  },
  "author": {
    "name": "Júlia Romero Remacha",
    "email": "jromerorem@uoc.edu"
  },
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.16.0",
    "autoprefixer": "^10.4.21",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.16.0",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.4.0",
    "posthtml": "^0.16.7",
    "posthtml-include": "^2.0.1",
    "rimraf": "^6.1.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "animate.css": "^4.1.1"
  }
}
